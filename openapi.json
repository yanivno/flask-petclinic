        "tags": ["owners"],
        "summary": "Delete an owner",
        "description": "Deletes a pet owner and all associated pets",
        "operationId": "deleteOwner",
        "parameters": [
          {
            "name": "ownerId",
            "in": "path",
            "description": "ID of owner to delete",
            "required": true,
            "schema": {
